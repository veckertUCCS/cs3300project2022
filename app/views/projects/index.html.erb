<body>
<div>
    <div class="container-fluid bg-dark text-white">
        <div class="row g-0">
            <div class="col-sm-1">
              <a class="btn btn-dark" href="/" role="button">Home</a>
            </div>
        </div>
    </div>
</div>

<div id="projects" class="pt-5">
  <% @projects.each do |project| %>
    <div class="container-fluid border border-primary rounded border-2 py-1"
      <%= render project %>
      <p>
        <%= link_to "Show This Project", project, :class => 'btn btn-dark btn-md px-4 gap-3' %>
      </p>
    </div>
  <% end %>
</div>

<% if user_signed_in? %>
  <div class="pt-2 px-3">
    <%= link_to "New Project", new_project_path, :class => 'btn btn-info btn-md px-4 gap-3'%>
  </div>
<% end %>

</body>